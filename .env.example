# ===========================================
# FlexiResume 环境变量配置模板
# FlexiResume Environment Variables Template
# ===========================================
#
# 复制此文件为 .env 并根据需要修改配置值
# Copy this file to .env and modify configuration values as needed
#

# ===========================================
# 百度统计配置
# Baidu Analytics Configuration
# ===========================================

# 是否启用百度统计 (true/false)
# Enable Baidu Analytics (true/false)
VITE_BAIDU_ENABLED=false

# 百度统计站点ID (必填，如果启用百度统计)
# Baidu Analytics Site ID (Required if Baidu Analytics is enabled)
VITE_BAIDU_SITE_ID=your_baidu_site_id

# 统计域名 (可选，默认使用当前域名)
# Analytics Domain (Optional, defaults to current domain)
VITE_BAIDU_DOMAIN=your-domain.com

# ===========================================
# Google Analytics 配置
# Google Analytics Configuration
# ===========================================

# 是否启用Google Analytics (true/false)
# Enable Google Analytics (true/false)
VITE_GOOGLE_ENABLED=false

# Google Analytics Measurement ID (必填，如果启用GA)
# Google Analytics Measurement ID (Required if GA is enabled)
# 示例: G-7LG0G58765 (请替换为您的实际测量ID)
VITE_GOOGLE_MEASUREMENT_ID=G-XXXXXXXXXX



# Firebase 配置 (可选，用于Firebase Analytics)
# Firebase Configuration (Optional, for Firebase Analytics)

# 是否使用Firebase Analytics (true/false，默认false)
# Use Firebase Analytics (true/false, default false)
VITE_GOOGLE_USE_FIREBASE=false

# 是否使用动态加载 (true/false，默认true)
# Use Dynamic Loading (true/false, default true)
VITE_GOOGLE_DYNAMIC_LOADING=true
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# ===========================================
# ELK Stack 配置
# ELK Stack Configuration
# ===========================================

# 是否启用ELK统计 (true/false)
# Enable ELK Analytics (true/false)
VITE_ELK_ENABLED=false

# ELK端点 (通过Nginx代理，替代Analytics Server)
# ELK Endpoint (Via Nginx proxy, replaces Analytics Server)
VITE_ELK_ENDPOINT=http://localhost:5000/api/analytics

# ELK批量大小 (默认10)
# ELK Batch Size (default 10)
VITE_ELK_BATCH_SIZE=10

# ELK刷新间隔 (毫秒，默认5000)
# ELK Flush Interval (milliseconds, default 5000)
VITE_ELK_FLUSH_INTERVAL=5000

# ===========================================
# 项目基础配置
# Project Basic Configuration
# ===========================================

# 项目名称
# Project Name
VITE_APP_NAME=FlexiResume

# 项目版本
# Project Version
VITE_APP_VERSION=1.0.0

# 构建时间 (自动生成，无需手动修改)
# Build Time (Auto-generated, no manual modification needed)
VITE_APP_BUILD_TIME=

# 是否启用调试模式
# Enable Debug Mode
VITE_DEBUG=false

# ===========================================
# CDN配置
# CDN Configuration
# ===========================================

# 是否启用CDN
# Enable CDN
VITE_CDN_ENABLED=true

# CDN基础URL列表 (用逗号分隔)
# CDN Base URLs (comma separated)
VITE_CDN_BASE_URLS=https://cdn.jsdelivr.net/gh/dedenLabs/flexiresume-static/,https://flexiresume-static.web.app/,https://dedenlabs.github.io/flexiresume-static/

# CDN健康检查超时时间 (毫秒)
# CDN Health Check Timeout (milliseconds)
VITE_CDN_HEALTH_CHECK_TIMEOUT=5000

# CDN健康检查测试路径
# CDN Health Check Test Path
VITE_CDN_HEALTH_CHECK_PATH=favicon.ico

# CDN排序策略 (speed/availability)
# CDN Sorting Strategy (speed/availability)
VITE_CDN_SORTING_MODE=speed

# ===========================================
# 静态路由配置
# Static Route Configuration
# ===========================================

# 静态路由页面名称 (用逗号分隔)
# Static Route Page Names (comma separated)
# 这些页面将在构建时生成对应的HTML文件
# These pages will generate corresponding HTML files during build
VITE_STATIC_ROUTE_PAGES=game,frontend,backend,cto,agent,contracttask,fullstack

# ===========================================
# 性能配置
# Performance Configuration
# ===========================================

# 是否启用懒加载
# Enable Lazy Loading
VITE_ENABLE_LAZY_LOADING=true

# 懒加载阈值 (像素)
# Lazy Loading Threshold (pixels)
VITE_LAZY_LOADING_THRESHOLD=100

# 是否启用预加载
# Enable Preloading
VITE_ENABLE_PRELOADING=true

# 预加载资源列表 (用逗号分隔)
# Preload Resources List (comma separated)
VITE_PRELOAD_RESOURCES=/images/avatar.webp,/images/flexi-resume.jpg

# ===========================================
# 主题配置
# Theme Configuration
# ===========================================

# 默认主题 (auto/light/dark)
# Default Theme (auto/light/dark)
VITE_DEFAULT_THEME=auto

# 是否启用主题切换动画
# Enable Theme Transitions
VITE_ENABLE_THEME_TRANSITIONS=true

# 主题切换动画持续时间 (毫秒)
# Theme Transition Duration (milliseconds)
VITE_THEME_TRANSITION_DURATION=300

# ===========================================
# API配置
# API Configuration
# ===========================================

# API基础URL
# API Base URL
VITE_API_BASE_URL=

# API超时时间 (毫秒)
# API Timeout (milliseconds)
VITE_API_TIMEOUT=10000

# API版本
# API Version
VITE_API_VERSION=v1
